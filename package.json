    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/acdb-team/google-sheets-to-json.git"
  },
  "bugs": {
    "url": "https://github.com/acdb-team/google-sheets-to-json/issues"
  },
  "name": "acdb-monorepo",
  "workspaces": [
    "packages/*"
  ],
